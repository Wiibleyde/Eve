{
    "name": "eve",
    "module": "index.ts",
    "scripts": {
        "format": "prettier --write \"**/*.{json,ts}\"",
        "lint": "eslint .",
        "generate": "prisma generate",
        "build-js": "bun build ./index.ts --minify --outdir ./dist --format esm --target node",
        "postbuild": "cp -r node_modules/.prisma dist/",
        "build": "npm run generate && npm run build-js && npm run postbuild",
        "start": "node dist/index.js"
    },
    "devDependencies": {
        "@eslint/js": "^9.27.0",
        "@types/bun": "^1.2.14",
        "eslint": "^9.27.0",
        "globals": "^16.2.0",
        "prettier": "3.5.3",
        "typescript-eslint": "^8.32.1"
    },
    "peerDependencies": {
        "typescript": "^5.8.3"
    },
    "private": true,
    "type": "module",
    "dependencies": {
        "@google/genai": "^1.0.1",
        "@prisma/client": "^6.8.2",
        "axios": "^1.9.0",
        "blagues-api": "^2.1.1",
        "cron": "^4.3.0",
        "discord.js": "^14.19.3",
        "dotenv": "^16.5.0",
        "jimp": "^1.6.0",
        "prisma": "^6.8.2"
    }
}
