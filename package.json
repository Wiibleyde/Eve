{
    "name": "eve",
    "module": "index.ts",
    "author": {
        "name": "Wiibleyde",
        "email": "nathan@bonnell.fr"
    },
    "version": "3.0.2",
    "description": "Eve is a Discord bot that provides various features such as image processing, AI interactions, and more.",
    "license": "GPL-2.0-only",
    "maintainers": [
        {
            "name": "Wiibleyde",
            "email": "nathan@bonnell.fr"
        }
    ],
    "scripts": {
        "format": "prettier --write \"**/*.{json,ts}\"",
        "lint": "eslint .",
        "generate": "prisma generate",
        "build-js": "bun build ./index.ts --outdir ./dist --format esm --target node",
        "postbuild": "cp -r node_modules/.prisma dist/ && cp -r assets dist/",
        "build": "npm run generate && npm run build-js && npm run postbuild",
        "start": "node dist/index.js"
    },
    "devDependencies": {
        "@eslint/js": "^9.37.0",
        "@types/bun": "^1.3.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "eslint": "^9.37.0",
        "globals": "^16.4.0",
        "prettier": "3.6.2",
        "typescript-eslint": "^8.46.0"
    },
    "peerDependencies": {
        "typescript": "^5.8.3"
    },
    "private": true,
    "type": "module",
    "dependencies": {
        "@google/genai": "^1.24.0",
        "@prisma/client": "^6.17.1",
        "axios": "^1.12.2",
        "blagues-api": "^2.1.1",
        "cors": "^2.8.5",
        "cron": "^4.3.3",
        "discord.js": "^14.23.2",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "ical.js": "^2.2.1",
        "jimp": "^1.6.0",
        "prisma": "^6.17.1"
    }
}
